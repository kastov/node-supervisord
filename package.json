{
  "name": "@kastov/node-supervisord",
  "version": "2.0.3",
  "description": "A Node.js library for communicating with the Supervisord XML-RPC API",
  "keywords": [
    "supervisor",
    "supervisord",
    "sdk",
    "typescript",
    "module",
    "addon"
  ],
  "homepage": "https://github.com/kastov/node-supervisord",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kastov/node-supervisord.git"
  },
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky",
    "prebuild": "rimraf dist",
    "format": "prettier --write .",
    "build": "tsc -b tsconfig.build.json",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@types/sax": "^1.2.7",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "rimraf": "^5.0.5",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.49.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-paths": "^1.1.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "src/**/*.{ts|test.ts}": [
      "eslint --max-warnings 0 \"src/**/*.ts\"",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "byte-base64": "^1.1.0",
    "got": "^14.4.5",
    "sax": "^1.4.1",
    "xmlbuilder2": "^3.1.1"
  }
}
